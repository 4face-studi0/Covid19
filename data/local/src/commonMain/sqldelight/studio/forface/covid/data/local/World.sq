import studio.forface.covid.domain.entity.Name;
import studio.forface.covid.domain.entity.WorldId;

CREATE TABLE world (
  _id INTEGER NOT NULL DEFAULT 0 PRIMARY KEY,
  id TEXT as WorldId NOT NULL,
  name TEXT as Name NOT NULL
);

-- Select
select:
  SELECT * FROM world
  LIMIT 1;

-- Insert
insert:
  INSERT OR REPLACE
  INTO world(id, name)
  VALUES (?, ?);

-- Delete
delete:
  DELETE FROM world;
